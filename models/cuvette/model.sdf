          <contact>
            <ode>
              <kp>100000</kp>
              <kd>1.0</kd>
              <contact_surface_layer>0.0015</contact_surface_layer>
            </ode>
          </contact>
        </surface>
      </collision>

      <inertial>
        <mass>0.015</mass>
        <inertia>
          <ixx>2.73e-06</ixx>
          <iyy>2.73e-06</iyy>
          <izz>3.91e-07</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
